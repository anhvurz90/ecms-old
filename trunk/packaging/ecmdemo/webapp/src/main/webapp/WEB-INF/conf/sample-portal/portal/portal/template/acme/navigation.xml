<node-navigation
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_2 http://www.gatein.org/xml/ns/gatein_objects_1_2"
    xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_2">
  <priority>1</priority>
  <page-nodes>
    <node>
      <uri>overview</uri>
      <name>overview</name>
      <label xml:lang="en">Overview</label>
      <label xml:lang="fr">Présentation</label>
      <label xml:lang="vi">Giới thiệu</label>
      <label xml:lang="pt-BR">Resumo</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::@owner@::overview</page-reference>
    </node>
    
    <node>
      <uri>news</uri>
      <name>news</name>
      <label xml:lang="en">News</label>
      <label xml:lang="fr">Actualités</label>
      <label xml:lang="vi">Tin tức</label>
      <label xml:lang="pt-BR">Notícias</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::@owner@::news</page-reference>
    </node>

    <node>
      <uri>events</uri>
      <name>events</name>
      <label xml:lang="en">Events</label>
      <label xml:lang="fr">Evénements</label>
      <label xml:lang="vi">Sự kiện</label>
      <label xml:lang="pt-BR">Eventos</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::@owner@::events</page-reference>
    </node>

    <node>
      <uri>newsletters</uri>
      <name>newsletters</name>
      <label xml:lang="en">Newsletters</label>
      <label xml:lang="fr">Lettres d'information</label>
      <label xml:lang="vi">Thư giới thiệu</label>
      <label xml:lang="pt-BR">Newsletters</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::@owner@::newsletters</page-reference>
    </node>

    <node>
      <uri>contactus</uri>
      <name>contactus</name>
      <label xml:lang="en">Contact Us</label>
      <label xml:lang="fr">Contactez Nous</label>
      <label xml:lang="vi">Liên hệ</label>
      <label xml:lang="pt-BR">Contate-nos</label>
      <page-reference>portal::@owner@::contactus</page-reference>
      <node>
        <uri>contactus/confirmation</uri>
        <name>confirmation</name>
        <label xml:lang="en">Contact Form Confirmation</label>
        <label xml:lang="fr">Confirmation de contact</label>
        <label xml:lang="vi">Xác nhận</label>
        <label xml:lang="pt-BR">Confirmação de Formulário de Contato</label>        
        <visibility>HIDDEN</visibility>
        <page-reference>portal::@owner@::confirmation</page-reference>
      </node>
    </node>

    <node>
      <uri>detail</uri>
      <name>detail</name>
      <label xml:lang="en">Details</label>
      <label xml:lang="fr">Détails</label>
      <label xml:lang="vi">Chi tiết</label>
      <label xml:lang="pt-BR">Detalhes</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::@owner@::detail</page-reference>
    </node>

    <node>
      <uri>printviewer</uri>
      <name>printviewer</name>
      <label xml:lang="en">Print Page</label>
      <label xml:lang="fr">Imprimer la page</label>
      <label xml:lang="vi">Trang in</label>
      <label xml:lang="pt-BR">Imprimir página</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::@owner@::printviewer</page-reference>
    </node>

    <node>
      <uri>searchResult</uri>
      <name>searchResult</name>
      <label xml:lang="en">Search</label>
      <label xml:lang="fr">Résultats de recherche</label>
      <label xml:lang="vi">Kết quả tìm kiếm</label>
      <label xml:lang="pt-BR">Procurar</label>
      <visibility>HIDDEN</visibility>
      <page-reference>portal::@owner@::searchResult</page-reference>
    </node>

    <node>
      <uri>newAccount</uri>
      <name>newAccount</name>
      <label xml:lang="en">New Account</label>
      <label xml:lang="fr">Nouveau compte</label>
      <label xml:lang="vi">Đăng ký</label>
      <label xml:lang="pt-BR">Nova conta</label>
      <visibility>HIDDEN</visibility>
      <page-reference>portal::@owner@::newAccount</page-reference>
    </node>

    <node>
      <uri>groupnavigation</uri>
      <name>groupnavigation</name>
      <label xml:lang="en">Group Navigation</label>
      <label xml:lang="fr">Navigation du Groupe</label>
      <label xml:lang="vi">Quản lý thanh điều hướng cho nhóm</label>
      <label xml:lang="pt-BR">Navegação de Grupos</label>      
      <visibility>SYSTEM</visibility>
      <page-reference>portal::@owner@::groupnavigation</page-reference>     
    </node>
    
    <node>
      <uri>portalnavigation</uri>
      <name>portalnavigation</name>
      <label xml:lang="en">Portal Navigation</label>
      <label xml:lang="fr">Navigation du Portail</label>
      <label xml:lang="vi">Quản lý thanh điều hướng chung</label>
      <label xml:lang="pt-BR">Navegação do Portal</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::@owner@::portalnavigation</page-reference>      
    </node>

  </page-nodes>
</node-navigation>